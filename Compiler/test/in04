/* 
 * One interface, one module
 * Tests: Arithmetic and bit operators
 */

    event open (string a1 , int a2 , unsigned int a3);

    int i = 2 ;
    int j = 3 ;
    int k = 4.0 ;
    double f = 3.3;

    byte b ;
    string s ;

    open(file, flags, mode) -> {
                    i = (23 * (-i)/j) - f - (45 % j) ;
                    i = s * 1 % f; 
                    i = b + s - f ;
                    b = j * f | i & j ;
                    s = s * s + 45 < k ;
                    j = i & (j | k) ^ i & (~j);
                    j = i & (f | k) ^ i & (~f);
                    k = k << (j >> i) ;
                    f = f << f ;
    } ;
